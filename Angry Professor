
#include<bits/stdc++.h>
using namespace std;

int main(){
    int q;
    cin>>q;
    for(int i=0;i<q;i++){
        int n,k,c=0;
        cin>>n>>k;
        int* a=new int[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
            if(a[i]<=0)
                c++;
        }
        if(k<=c){
            cout<<"NO"<<endl;
        }
        else{
            cout<<"YES"<<endl;
        }
    }
    return 0;
}

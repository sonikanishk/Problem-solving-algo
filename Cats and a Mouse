#include <bits/stdc++.h>
using namespace std;


int main()
{

    int q;
    cin>>q;
    for(int i=0;i<q;i++){
        int x,y,z;
        cin>>x>>y>>z;
        int d1=z-x;
        if(d1<0){
            d1=-d1;
        }
        int d2=z-y;
        if(d2<0){
            d2=-d2;
        }
        if(d1==d2){
            cout<<"Mouse C"<<endl;
        }
        else if(d1>d2){
            cout<<"Cat B"<<endl;
        }
        else{
            cout<<"Cat A"<<endl;
        }
    }

    return 0;

}
